@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Evaluate</title>
    <link href="~/assets/css/common.css" rel="stylesheet" />
    <link href="~/assets/css/teacher.css" rel="stylesheet" />
    <link href="~/assets/css/table.css" rel="stylesheet" />
</head>
<body>
    <div class="banner_one"></div>
    @Html.Partial("TeacherNav", 3)

<div class="section">
    <div class="ad">
        <div class="container courselist myevaluate">
            <div class="page-title">
                <h1 class="content_title">Score</h1>
                <div class="divider"></div>
            </div>
            <div class="">
                <div class="courselist_table_container">
                    <table class="course_yueke">
                        <colgroup>
                            <col width="15%" />
                            <col width="13%" />
                            <col width="15%" />
                            <col width="12%" />
                            <col width="10%" />
                            <col width="20%" />
                        </colgroup>
                        <thead>
                            <tr>
                                <th>Class Type</th>
                                <th>Class Level</th>
                                <th>Subject</th>
                                <th>Class Mode</th>
                                <th>Progress</th>
                                <th>Evaluation</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                    <div class="page"></div>
                </div>
                <div class="classhour hidden">
                    <table class="course_yueke">
                        <thead>
                            <tr>
                                <th>Lesson times</th>
                                <th style="width: 113px;">School teacher</th>
                                @*<th style="width: 113px;">School manager</th>*@
                                <th>Booked/Reducible</th>
                                <th>Class time</th>
                                <th>Duration</th>
                                <th>Status</th>
                                <th>Operation</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                    <div class="page"></div>
                </div>
                <div class="orderlist hidden">
                    <div style="width: 100%; height: 55px;">
                        <ul class="nav-tabs tab">
                            <li data="1" style="font-size: 30px; padding: 0 5px; width: auto; height: 50px; line-height: 50px; background-image: initial;">Student Feedback</li>
                            <li data="0" style="font-size: 30px; padding: 0 5px; width: auto; height: 50px; line-height: 50px; background-image: initial; ">Teacher Comments</li>
                        </ul>
                    </div>
                    <div class="order-list" style="height: 500px">
                    </div>
                    <div style="width: 100%; height: 60px;">
                        <div class="page"></div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
    
    <script src="~/js/jquery-1.8.3.min.js"></script>
    <script src="~/js/base.js"></script>
    <script src="~/js/jquery.cookie.js"></script>
    <script src="~/js/layer/layer.js"></script>
    <script src="~/js/common.js"></script>
    <script src="~/js/teachermember.js"></script>
    <script>
        $(function () {
            if (window.top && window.top.updateContentFrameHeight) {
                window.top.updateContentFrameHeight(1200);
            }
            member.myEvaluateReady();
        });
    </script>
</body>
</html>
