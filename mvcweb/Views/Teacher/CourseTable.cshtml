<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CourseTable</title>
    <link href="~/assets/css/common.css" rel="stylesheet" />
    <link href="~/js/laydate/need/laydate.css" rel="stylesheet" />
    <link href='~/js/fullcalendar/fullcalendar.css' rel='stylesheet' />
    <link href='~/js/fullcalendar/fullcalendar.print.css' rel='stylesheet' media='print' />
    <link href="~/assets/css/teacher.css" rel="stylesheet" />
</head>
<body>
    <div class="banner_one"></div>
    @Html.Partial("TeacherNav", 2)
    <div class="section">
        <div class="ad ad-1">
            <div class="container">
                <div class="page-title">
                    <h1>COURSE MANAGEMENT</h1>
                    <div class="divider"></div>
                </div>
                <div class="free-course">
                    <div class="coursetable flt" style="margin-left: 11px;">
                        <div class="fullcalender">
                        </div>
                        <div class="courseform hidden">
                            <table>
                                <tr>
                                    <td>课程类型：</td>
                                    <td>
                                        <div id="kclx"></div>
                                    </td>
                                </tr>
                                <tr>
                                    <td>课程级别：</td>
                                    <td>
                                        <div id="kcjb"></div>
                                    </td>
                                </tr>
                                <tr>
                                    <td>课程名称：</td>
                                    <td>
                                        <div id="kcmc"></div>
                                    </td>
                                </tr>
                                <tr>
                                    <td>上课老师：</td>
                                    <td>
                                        <div id="skls"></div>
                                    </td>
                                </tr>
                                <tr>
                                    <td>上课模式：</td>
                                    <td>
                                        <div id="skms"></div>
                                    </td>
                                </tr>
                                <tr>
                                    <td>上课时间：</td>
                                    <td>
                                        <div id="sksj"></div>
                                    </td>
                                </tr>
                                <tr>
                                    <td>上课时长：</td>
                                    <td>
                                        <div id="sksc"></div>
                                    </td>
                                </tr>
                                <tr>
                                    <td>课次：</td>
                                    <td>
                                        <div id="kc"></div>
                                    </td>
                                </tr>
                            </table>

                            <div class="oper">
                                <a class="btn" type="1" target="_blank">进入教室</a>
                                <a class="btn" type="2">取消预约</a>
                                <a class="btn" type="3" target="_blank">视频回放</a>
                                <a class="btn" type="4" target="_blank">下载课件</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--查看课件-->
    <div class="lessonaccess hidden">
        <table class="course_yueke">
            <thead>
                <tr>
                    <td>序号</td>
                    <td>课次号</td>
                    <td>课次名称</td>
                    <td>课件名称</td>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>

    
    
    <script src="~/js/jquery-1.8.3.min.js"></script>
    <script src="~/js/base.js"></script>
    <script src="~/js/jquery.cookie.js"></script>
    <script src="~/js/layer/layer.js"></script>
    <script src="~/js/laydate/laydate.js"></script>
    <script src='~/js/fullcalendar/lib/moment.min.js'></script>
    <script src="~/js/fullcalendar/fullcalendar.js"></script>
    <script src="~/js/fullcalendar/lang/zh-cn.js"></script>
    <script src="~/js/common.js"></script>
    <script src="~/js/teachermember.js"></script>
    <script>
        $(function () {
            if (window.top && window.top.updateContentFrameHeight) {
                window.top.updateContentFrameHeight(1200);
            }
            member.myCourseListReady();
        });
    </script>
</body>
</html>
