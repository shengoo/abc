<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>ABC在线英语</title>
    <link href="~/static/css/Purchase.css" rel="stylesheet" />
    <script>
        window.parent.$('#ContentFrame').css({
            height: 1300
        });
    </script>
</head>
<body>
<div class="banner_one"></div>
    @Html.Partial("UserCenterNav", 2)
    <div class="section">
        <div class="ad ad-1">
            <div class="container">
                <div class="page-title">
                    <h1>COURSE MANAGEMENT</h1>
                    <div class="divider"></div>
                    @Html.Partial("PurchaseTabs", new ViewDataDictionary { { "index", 0 } })
                </div>
                <div class="free-course coursecard">
                    <div class="each-media">
                        <table class="extb">
                            <thead>
                                <tr class="odd add">
                                    <td class="opp">序号</td>
                                    <td>课程卡编号</td>
                                    <td>课程卡类型</td>
                                    <td>课程卡名称</td>
                                    <td>课程名称</td>
                                    <td>课程类型</td>
                                    <td>卡次总数</td>
                                    <td>剩余次数</td>
                                    <td>截止有限期</td>
                                </tr>
                            </thead>
                            <tbody class="coursecardbody"></tbody>
                            
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="~/js/jquery-1.8.3.min.js"></script>
    <script src="~/js/base.js"></script>
    <script src="~/js/jquery.cookie.js"></script>
    @*<script src="~/js/layer/layer.js"></script>*@
    <script src="~/js/common.js"></script>
    <script src="~/js/member.js" type="text/javascript"></script>
    <script>
        $(function () {
            window.top.$('#ContentFrame').css({
                height: 1400
            });
            member.myCourseCardReady();
        });
    </script>
</body>
</html>
