        .maskcontent {
            text-align: left;
            background-color: #fff;
            font-size: 14px;
            line-height: 1.5em;
            text-indent: 4em;
            height: auto;
            min-height: 96px;
        }

        .masktip .btn {
            padding: 3px 6px;
            margin-bottom: 10px;
            margin-right: 20px;
            width: 50px;
            height: 20px;
        }

        .maskfooter {
            background-color: #00ffff;
        }

        .clearfix:after {
            content: "\200B";
            display: block;
            height: 0;
            clear: both;
        }

        .clearfix {
            *zoom: 1;
        }
    </style>
</head>
